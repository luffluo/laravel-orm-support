{
    "name": "luffluo/laravel-orm-support",
    "description": "Extend Laravel ORM",
    "license": "MIT",
    "authors": [
        {
            "name": "luffluo",
            "email": "luffluo@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "~5.5|~6.0|~7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Luffluo\\LaravelOrmSupport\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": ["Luffluo\\LaravelOrmSupport\\ServiceProvider"]
        }
    }
}
